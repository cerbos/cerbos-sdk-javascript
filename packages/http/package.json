  "keywords": [
    "Cerbos",
    "authorization",
    "access control",
    "roles",
    "permissions",
    "policy",
    "security",
    "role-based access control",
    "RBAC",
    "attribute-based access control",
    "ABAC",
    "policy decision point",
    "PDP",
    "HTTP",
    "REST",
    "browser"
  ],
  "dependencies": {
    "@cerbos/core": "^0.19.0",
    "qs": "^6.13.1"
  },
  "devDependencies": {
    "@types/qs": "6.9.18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
